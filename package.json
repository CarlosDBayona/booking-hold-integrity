    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.3",
    "ioredis-mock": "^8.9.0",
    "supertest": "^7.1.1",
    "testcontainers": "^10.24.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
